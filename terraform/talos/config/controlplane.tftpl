cluster:
  controllerManager:
    extraArgs:
      bind-address: 0.0.0.0
  etcd:
    extraArgs:
      listen-metrics-urls: http://0.0.0.0:2381
  scheduler:
    extraArgs:
      bind-address: 0.0.0.0
machine:
  network:
    interfaces:
    - interface: eth0
      vip:
        ip: ${vip}
  certSANs:
  - ${vip}
